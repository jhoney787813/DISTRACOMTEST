@model List<MVCSERIESTEST.Models.sp_GetSeriesUser_Result>
@{
    ViewBag.Title = "Listado de registros por USERNAME";
}

<h2>@ViewBag.Title</h2>

<div class="row" style="text-align:right;padding:5px;">
    <a href="~/Series" class="btn btn-primary">Nueva Busqueda</a>
</div>


<div class="row">
    <div class="col-md-12">
        <table class="table">
            <tr>
                <th>#</th>
                <th>User Name</th>
                <th>Creation Date</th>
                <th>Series Split</th>
                <th></th>
            </tr>

            @foreach (var oElemento in Model)
            {
                <tr>
                    <td>@oElemento.IDSeries</td>
                    <td>@oElemento.UserName</td>
                    <td>@oElemento.CreationDate</td>
                    <td>@oElemento.SeriesSplit</td>
                 
                </tr>

            }
        </table>
    </div>

</div>

